// ch04_06

#include <iostream>
#include <string>
using namespace std;

class Employee
{
	string name;
	int age;
	int salary;

public:
	int print(string n, int a, int s)
	{
		name = n;
		age = a;
		salary = s;

		cout << name << "\n" << age << "\n" << salary;

		return 0;
	}
};

int main()
{
	Employee e;

	cout << "Employee1:" << "\n";
	cout << e.print("김철수", 38, 2000000) << endl;

	return 0;
}
